import java.util.*;
public class Add
{
public status void main(String[] arg)
{
int a,b,c;
Scanner s=new Scanner(System.in)
System.out.println("Enter the given number is a");
a=s.nextInt();
System.out.println("Enter the given number is b");
b=s.nextInt();
c=a+b;
